{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        
        "sourceMap": false,
        "noImplicitAny": false,
        "removeComments": false,
        "declaration": false,
        "outDir": "typeScriptConvertedFiles",
        "types": ["jasmine","jasminewd2", "node"]
    },
    "exclude": [
        "node_modules",
        "typeScriptConvertedFiles"
    ]
}